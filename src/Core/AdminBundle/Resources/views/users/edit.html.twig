{% extends 'CoreAdminBundle:admin:layout.html.twig' %}
{% block content %}

        <div id="title">
            <h1>Usuarios :: Editar</h1>
        </div>

        <div>
            <table>
                <tr align="center">
                    <th colspan="3">
                        {% if msg %}
                        <div id="msg">{{ msg }}</div>
                        {% endif %}
                    </th>
                </tr>
                <tr align="left">
                    <th width="20%"></th>
                    <th width="60%"> {{ form(form) }} </th>
                    <th width="20%"></th>
                </tr>
            </table>
        </div>

{% endblock %}